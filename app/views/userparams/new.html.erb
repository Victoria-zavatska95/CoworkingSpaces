<%= form_for @userparam, url: userparams_path do |f| %>
  Firsname: <%= f.text_field :firstname %><br />
  Lastname: <%= f.text_field :lastname %><br />
  Age: <%= f.text_field :age %><br />
  Hometown: <%= f.text_field :hometown %><br />
  Occupation: <%= f.text_field :occupation %><br />
  <%= f.submit "Create" %>
<% end %>


      